{
  "name": "tetris",
  "version": "1.0.0",
  "description": "tetris game application",
  "author": "",
  "homepage": "https://pik6c.tech/tetris",
  "main": "main.js",
  "scripts": {
    "dev": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.tetris.konoha",
    "productName": "TETRIS",
    "files": [
      "**/*"
